{
    "name": "@jpapini/eslint-config",
    "version": "1.4.9",
    "description": "Custom ESLint configuration for JavaScript and TypeScript projects.",
    "keywords": [
        "jpapini",
        "eslint-config",
        "tools",
        "lint",
        "eslint",
        "nestjs",
        "react"
