    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jpapini/shared-packages.git",
        "directory": "packages/eslint-config"
    },
    "license": "MIT",
    "author": "Julien Papini <julien.papini@gmail.com> (https://github.com/jpapini)",
    "type": "commonjs",
    "exports": {
