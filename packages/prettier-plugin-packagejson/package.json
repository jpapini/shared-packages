{
    "name": "@jpapini/prettier-plugin-packagejson",
    "version": "1.0.1",
    "description": "Custom Prettier plugin for formatting package.json files.",
    "keywords": [
        "jpapini",
        "prettier-plugin",
        "tools",
        "lint",
        "prettier",
        "packagejson"
    ],
    "homepage": "https://github.com/jpapini/shared-packages/tree/main/packages/prettier-plugin-packagejson#readme",
    "bugs": {
        "url": "https://github.com/jpapini/shared-packages/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jpapini/shared-packages.git",
        "directory": "packages/prettier-plugin-packagejson"
    },
    "license": "MIT",
    "author": "Julien Papini <julien.papini@gmail.com> (https://github.com/jpapini)",
    "type": "module",
    "exports": {
        ".": "./index.js"
    },
    "files": [
        "*.js"
    ],
    "scripts": {
        "clean": "rimraf .turbo node_modules/.cache",
        "reset": "pnpm run clean; rimraf node_modules"
    },
    "dependencies": {
        "detect-indent": "7.0.1",
        "detect-newline": "4.0.1",
        "git-hooks-list": "4.1.1",
        "is-plain-obj": "4.1.0",
        "semver": "7.7.2",
        "sort-object-keys": "1.1.3"
    },
    "devDependencies": {
        "prettier": "3.6.2"
    },
    "peerDependencies": {
        "prettier": "3"
    },
    "packageManager": "pnpm@10.14.0",
    "engines": {
        "node": "22",
        "pnpm": "10"
    },
    "volta": {
        "node": "22.18.0",
        "pnpm": "10.14.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
